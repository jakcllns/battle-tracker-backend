{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
    },
    "overrides": [
        {
            "files": ["*.js"],
            "processor": "@graphql-eslint/graphql"
        },
        {
          "files": ["*.graphql"],
          "parser": "@graphql-eslint/eslint-plugin",
          "plugins": ["@graphql-eslint"],
          "rules": {
            "@graphql-eslint/known-type-names": "error"
          }
        }
      ]
}
